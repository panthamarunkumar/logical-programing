#include<iostream>
#include <cstring>
using namespace std;

int main()
{
    char s[88];
    cout<<"enter tghe sting";
    gets(s);
    for(int i=0;i<strlen(s);i++)
    {
         if(s[i]==" ")
         {
             for(int j=0;i<strlen(s);j++)
             {
                 s[j]=s[j+1];
             }
         }
    }
    for(int i=0;i<strlen(s);i++)
    {
        cout<<s[i];
    }
}
